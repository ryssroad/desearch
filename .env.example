# Пример файла .env
# Скопируйте этот файл в .env и заполните необходимые значения

# Выбор API для Twitter
# true - использовать RapidAPI, false - использовать официальный Twitter API
USE_RAPID_API=true

# Ключ RapidAPI для Twitter
# Получите ключ на https://rapidapi.com/omarmhaimdat/api/twitter154
RAPID_API_KEY=your_rapid_api_key_here

# Bearer токен официального Twitter API (опционально, если USE_RAPID_API=false)
# TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Другие переменные окружения для Bittensor
# APIFY_API_KEY=your_apify_key_here
